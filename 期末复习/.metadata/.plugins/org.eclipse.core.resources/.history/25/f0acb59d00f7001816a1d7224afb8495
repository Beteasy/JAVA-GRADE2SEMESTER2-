package chicken;

public class Chicken {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int x=1; x<100; x++) {
			for(int y=1; y<100; y++) {
				for(int z=1; z<100; z++) {
					if(x+y+z==100 && 5x+3y+z/3==100 && x!=0 && y!=0 && z!=0) {
						System.out.println(x,y,z);
					}
				}
			}
		}
	}

}
